<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CheckupItemMapper">
	
	<resultMap type="CheckupItem" id="resultItems">
	    <id column="CHECKUPITEM_ID" property="CHECKUPITEM_ID"/>
	    <result column="SUBGROUP" property="SUBGROUP"/>
	    <result column="NAME" property="NAME"/>
	    <result column="FEATURES" property="FEATURES"/>
	    <result column="FREQUENCY" property="FREQUENCY"/>
	    <result column="STATUS" property="STATUS"/>
	    <result column="ORIGINATE" property="ORIGINATE"/>
	    <result column="DESCRIPTION" property="DESCRIPTION"/>
	    <result column="GENERATEDTIME" property="GENERATEDTIME"/>
	    <result column="WORKER" property="WORKER"/>
	    
	    <association property="checkupPackage" javaType="CheckupPackage">
			<id column="CHECKUPPACKAGE_ID" property="CHECKUPPACKAGE_ID"/>
		    <result column="WORKER" property="WORKER"/>
		    <result column="GENERATEDTIME" property="GENERATEDTIME"/>
		    <result column="EFFECTIVEFROM" property="EFFECTIVEFROM"/>
		    <result column="EXPIREON" property="EXPIREON"/>
		    <result column="STATUS" property="STATUS"/>
	    </association>
	</resultMap>
	
	<resultMap type="CheckupItem" id="resultItemsForUsers">
		<id column="CHECKUPITEM_ID" property="CHECKUPITEM_ID"/>
	    <result column="SUBGROUP" property="SUBGROUP"/>
	    <result column="NAME" property="NAME"/>
	    <result column="FEATURES" property="FEATURES"/>
	    <result column="FREQUENCY" property="FREQUENCY"/>
	    <result column="STATUS" property="STATUS"/>
	    <result column="ORIGINATE" property="ORIGINATE"/>
	    <result column="DESCRIPTION" property="DESCRIPTION"/>
	    <result column="GENERATEDTIME" property="GENERATEDTIME"/>
	    <result column="WORKER" property="WORKER"/>
	    <result column="REVISION" property="REVISION"/>
	    <result column="SYSFLAG" property="SYSFLAG"/>
	    
	    
		<association property="users" javaType="User">
			<id column="USERS_ID" property="USERS_ID"/>
		    <result column="USERNAME" property="USERNAME"/>
		    <result column="SEX" property="SEX"/>
	    </association>
	</resultMap>
	
	<!--表名 -->
	<sql id="tableName">
		TB_CHECKUPITEM
	</sql>
	
	<!-- 字段 -->
	<sql id="Field">
		SUBGROUP,	
		NAME,	
		FEATURES,	
		FREQUENCY,	
		STATUS,	
		ORIGINATE,	
		DESCRIPTION,	
		GENERATEDTIME,	
		WORKER,	
		REVISION,	
		SYSFLAG,	
		CHECKUPITEM_ID
	</sql>
	
	<!-- 字段值 -->
	<sql id="FieldValue">
		#{SUBGROUP},	
		#{NAME},	
		#{FEATURES},	
		#{FREQUENCY},	
		#{STATUS},	
		#{ORIGINATE},	
		#{DESCRIPTION},	
		#{GENERATEDTIME},	
		#{WORKER},	
		#{REVISION},	
		#{SYSFLAG},	
		#{CHECKUPITEM_ID}
	</sql>
	
	<!-- 通过checkupPackageId获取数据 -->
	<select id="findAllById" parameterType="int" resultMap="resultItemsForUsers">
		SELECT 
		  * 
		FROM
		  TB_CHECKUPITEM ci,
		  TB_CHECKUPPACKAGE cp,
		  sys_user u 
		WHERE ci.`CHECKUPPACKAGE_ID` = cp.`CHECKUPPACKAGE_ID` 
		  AND ci.`USER_ID` = u.`USER_ID` 
		  AND cp.`CHECKUPPACKAGE_ID` = #{cp.CHECKUPPACKAGE_ID}
	</select>
	
	<!-- 新增-->
	<insert id="save" parameterType="pd">
		insert into 
	<include refid="tableName"></include>
		(
	<include refid="Field"></include>
		) values (
	<include refid="FieldValue"></include>
		)
	</insert>
	
	<!-- 删除-->
	<delete id="delete" parameterType="pd">
		delete from
		<include refid="tableName"></include>
		where 
			CHECKUPITEM_ID = #{CHECKUPITEM_ID}
	</delete>
	
	<!-- 修改 -->
	<update id="edit" parameterType="pd">
		update
		<include refid="tableName"></include>
		set 
			SUBGROUP = #{SUBGROUP},
			NAME = #{NAME},
			FEATURES = #{FEATURES},
			FREQUENCY = #{FREQUENCY},
			STATUS = #{STATUS},
			ORIGINATE = #{ORIGINATE},
			DESCRIPTION = #{DESCRIPTION},
			GENERATEDTIME = #{GENERATEDTIME},
			WORKER = #{WORKER},
			REVISION = #{REVISION},
			SYSFLAG = #{SYSFLAG},
		CHECKUPITEM_ID = CHECKUPITEM_ID
		where 
		CHECKUPITEM_ID = #{CHECKUPITEM_ID}
	</update>
	
	<!-- 通过ID获取数据 -->
	<select id="findById" parameterType="pd" resultType="pd">
		select 
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 
			CHECKUPITEM_ID = #{CHECKUPITEM_ID}
	</select>
	
	<!-- 列表 -->
	<select id="datalistPage" parameterType="page" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 1=1
		<if test="pd.keywords!= null and pd.keywords != ''"><!-- 关键词检索 -->
			and
				(
				<!--	根据需求自己加检索条件
					字段1 LIKE CONCAT(CONCAT('%', #{pd.keywords}),'%')
					 or 
					字段2 LIKE CONCAT(CONCAT('%', #{pd.keywords}),'%') 
				-->
				)
		</if>
	</select>
	
	<!-- 列表(全部) -->
	<select id="listAll" parameterType="pd" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
	</select>
	
	<!-- 批量删除 -->
	<delete id="deleteAll" parameterType="String">
		delete from
		<include refid="tableName"></include>
		where 
			CHECKUPITEM_ID in
		<foreach item="item" index="index" collection="array" open="(" separator="," close=")">
                 #{item}
		</foreach>
	</delete>
	
	<!-- shouxin -->
</mapper>